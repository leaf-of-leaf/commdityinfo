<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
  PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
  "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<!-- mybatis-generator的核心配置文件 -->
<generatorConfiguration>
	
  <classPathEntry location="E:\javaEE\dingding\software\apache-maven-3.3.9\repository\mysql\mysql-connector-java\8.0.19\mysql-connector-java-8.0.19.jar" />
  <context id="DB2Tables" targetRuntime="MyBatis3">

  <!-- 生成一对一配置 -->
  <plugin type="cc.bandaotixi.plugins.OneToOnePlugin"></plugin>
  <!-- 生成一对多配置 -->
  <plugin type="cc.bandaotixi.plugins.OneToManyPlugin"></plugin>


    <!--自动生成的bean对象，是否实现序列化接口-->
  	<plugin type="org.mybatis.generator.plugins.SerializablePlugin"></plugin>
      <!--自动生成的bean对象，是否重写ToString()-->
    <plugin type="org.mybatis.generator.plugins.ToStringPlugin"></plugin>
  	<commentGenerator>
  		 <property name="suppressDate" value="true"/>
<!--        解决方法1，注释下面-->
          <property name="suppressAllComments" value="true"/>
  	</commentGenerator>
  
  	<!-- 注意！如果要重新生成pojo类，务必改成项目组数据库，用户名密码 -->
    <jdbcConnection driverClass="com.mysql.cj.jdbc.Driver"
        connectionURL="jdbc:mysql://47.115.1.253:3306/commdityinfo?serverTimezone=Asia/Shanghai"
        userId="root"
        password="kuangjie">
      <property name="nullCatalogMeansCurrent" value="true" />
    </jdbcConnection>
    
	<!--指定生成的类型为java类型，避免数据库中number等类型字段 -->
    <javaTypeResolver >
      <property name="forceBigDecimals" value="false" />
    </javaTypeResolver>

	<!--自动生成的实体的存放包路径 -->
    <javaModelGenerator targetPackage="com.kj.commdityinfo.bean" targetProject="./src/main/java">
      <property name="enableSubPackages" value="true" />
      <property name="trimStrings" value="true" />
    </javaModelGenerator>

	<!--自动生成的*Mapper.xml文件存放路径 -->
    <sqlMapGenerator targetPackage="mapper"  targetProject="./src/main/resources">
      <property name="enableSubPackages" value="true" />
    </sqlMapGenerator>

	<!--自动生成的*Mapper.java存放路径 -->
    <javaClientGenerator type="XMLMAPPER" targetPackage="com.kj.commdityinfo.mapper"  targetProject="./src/main/java">
      <property name="enableSubPackages" value="true" />
    </javaClientGenerator>

	<!-- 映射配置 -->
	<table tableName="iteminfo" domainObjectName="ItemInfo">
        <columnOverride column="content" javaType="java.lang.String" jdbcType="VARCHAR"></columnOverride>
    </table>
	<table tableName="user" domainObjectName="User">
        <oneToOne mappingTable="role" column="id" joinColumn="role_id" />
    </table>
	<table tableName="role" domainObjectName="Role"></table>
<!--	<table tableName="cms_article" domainObjectName="Article">-->
<!--		<columnOverride column="content" javaType="java.lang.String" jdbcType="VARCHAR"></columnOverride>-->
<!--	</table>-->
  </context>
</generatorConfiguration>
